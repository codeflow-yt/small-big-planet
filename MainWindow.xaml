        <Canvas x:Name="cnv_Canvas" Background="#FF4E458D">
            <Ellipse x:Name="elp_BigPlanet" Fill="#FF473C3C" Height="100" Stroke="White" Width="100" Canvas.Left="107" Canvas.Top="159"/>
            <Ellipse x:Name="elp_SmallPlanet" Fill="#FFE8DBD6" Height="24" Stroke="White" Width="24" Canvas.Left="247" Canvas.Top="27"/>
        </Canvas>

    </Grid>
</Window>
